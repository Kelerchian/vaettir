{
  "name": "vaettir-react",
  "version": "0.1.2",
  "type": "module",
  "main": "./dist/commonjs/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "cd ../vaettir && npm link && npm run clean && npm ci && npm run build",
    "dev:unlink": "npm unlink vaettir && npm install",
    "dist": "npm run clean && npm run build && npm run test",
    "build": "tshy",
    "clean": "rimraf ./dist",
    "test": "jest",
    "lint": "npx eslint src/**/*.ts",
    "lint:strict": "npm run lint -- --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run dist"
  },
  "homepage": "https://github.com/Kelerchian/vaettir/tree/master/vaettir-react",
  "author": "Alan Darmasaputra <kelerchian@gmail.com> (https://valand.dev/)",
  "license": "MIT",
  "description": "",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tshy": "^1.7.0",
    "typescript": "^5.2.2"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "types": "./dist/commonjs/index.d.ts",
  "dependencies": {
    "systemic-ts-utils": "^0.2.2",
    "vaettir": "^0.1.3"
  }
}
